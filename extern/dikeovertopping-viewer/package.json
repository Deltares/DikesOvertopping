{
  "private": true,
  "name": "deltares-bm-overslag",
  "scripts": {
    "start": "nuxt",
    "build": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --max-warnings 0 ."
  },
  "dependencies": {
    "1-liners": "^0.4.0",
    "axios": "^0.18.0",
    "fast-deep-equal": "^2.0.1",
    "nuxt": "^2.0.0",
    "vuex-map-fields": "^1.3.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "dotenv-safe": "^6.1.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-modify-css-urls": "^2.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "semi": 1,
      "no-debugger": 1,
      "no-console": 1,
      "vue/html-self-closing": 0,
      "vue/max-attributes-per-line": 0,
      "vue/require-v-for-key": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/deltares-bm-overslag.git"
  },
  "author": "Selwyn <talk@selwyn.cc> (https://selwyn.cc/)"
}
